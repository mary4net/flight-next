  /hotels/{hotel_id}/bookings:
    put:
      tags:
        - Hotels
        - '[hotel_id]'
        - bookings
      summary: Cancel booking
      description: >-
        This method allows cancellation of booking by hotel owner.


        Input: booking id.


        Output: message indicating how the cancellation went.


        This API endpoint allows users to cancel an existing booking by
        specifying the booking ID and the hotel ID. The request will update the
        status of the booking, marking it as cancelled.
      operationId: cancelBooking
      parameters:
        - name: bookingId
          in: query
          schema:
            type: string
            example: '1'
      responses:
        '200':
          description: Cancel booking
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Date:
              schema:
                type: string
                example: Sun, 09 Mar 2025 00:10:39 GMT
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: Booking cancelled successfully.
              examples:
                Cancel booking:
                  value:
                    message: Booking cancelled successfully.
      security:
        - {}
    parameters:
      - name: hotel_id
        in: path
        required: true
        schema:
          type: string
          example: '1'
