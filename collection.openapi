  /flights/desti:
    get:
      tags:
        - Destinations
      summary: auto-complete //need to update
      description: >-
        Auto-complete the ciy that the user is inputing. Also provides the
        airports of that city.
      operationId: autoCompleteNeedToUpdate
      parameters:
        - name: destination
          in: query
          schema:
            type: string
            example: Shang
          description: case sensitive, must be in Title Case.
      responses:
        '200':
          description: auto fill city
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Date:
              schema:
                type: string
                example: Sat, 08 Mar 2025 21:05:32 GMT
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
          content:
            application/json:
              schema:
                type: object
                properties:
                  matchingAirports:
                    type: array
                    items:
                      type: object
                      properties:
                        city:
                          type: string
                          example: Shanghai
                        code:
                          type: string
                          example: PVG
                        country:
                          type: string
                          example: China
                        id:
                          type: string
                          example: 42fb9788-abab-4395-be2c-0ee891a161d6
                        name:
                          type: string
                          example: Shanghai Pudong International Airport
                    example:
                      - city: Shanghai
                        code: PVG
                        country: China
                        id: 42fb9788-abab-4395-be2c-0ee891a161d6
                        name: Shanghai Pudong International Airport
                  matchingCities:
                    type: array
                    items:
                      type: object
                      properties:
                        city:
                          type: string
                          example: Shanghai
                        country:
                          type: string
                          example: China
                    example:
                      - city: Shanghai
                        country: China
              examples:
                auto fill city:
                  value:
                    matchingAirports:
                      - city: Shanghai
                        code: PVG
                        country: China
                        id: 42fb9788-abab-4395-be2c-0ee891a161d6
                        name: Shanghai Pudong International Airport
                    matchingCities:
                      - city: Shanghai
                        country: China
  /hotels/{hotel_id}/rooms:
    get:
      tags:
        - Hotels
        - '[hotel_id]'
        - rooms
      summary: Filter rooms
      description: >-
        This API endpoint allows users to filter available rooms at a specific
        hotel within a given date range. The request returns a list of rooms
        with details such as availability, price, and amenities during the
        specified time period.
      operationId: filterRooms
      parameters:
        - name: dateStart
          in: query
          schema:
            type: string
            example: '2024-10-11'
        - name: dateEnd
          in: query
          schema:
            type: string
            example: '2024-11-11'
      responses:
        '200':
          description: Filter rooms as user / Filter rooms as owner
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Date:
              schema:
                type: string
                example: Sat, 08 Mar 2025 23:02:43 GMT
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    amenities:
                      type: array
                      items:
                        type: string
                        example: WiFi
