    post:
      tags:
        - Hotels
        - '[hotel_id]'
        - rooms
      summary: Create room
      description: >-
        This API endpoint is used to create a new room for a specific hotel. The
        request allows you to specify the room type, amenities, price per night,
        and associated images for the room.
      operationId: createRoom
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                amenities:
                  type: array
                  items:
                    type: string
                    example: WiFi
                  example:
                    - WiFi
                    - Air Conditioning
                    - TV
                    - Mini Bar
                images:
                  type: array
                  items:
                    type: string
                    example: room1.jpg
                  example:
                    - room1.jpg
                    - room2.jpg
                pricePerNight:
                  type: number
                  example: 100
                type:
                  type: string
                  example: '101'
