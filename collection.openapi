                        income: 600.
                      updatedAt: '2025-03-09T00:05:20.233Z'
                      userId: 2
    parameters:
      - name: nid
        in: path
        required: true
        schema:
          type: string
          example: '1'
  /notifications:
    get:
      tags:
        - Notifications
      summary: get notification list
      description: >-
        return the whole notification list,


        or filter the unread notifications only, if parameter `unreadOnly` is
        true.
      operationId: getNotificationList
      parameters:
        - name: unreadOnly
          in: query
          schema:
